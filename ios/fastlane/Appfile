# Git repo for certificates
git_url("https://github.com/nabeelanasaritsaoirse/ios-certificates.git")

# Store signing assets in Git
storage_mode("git")

# Default type for match (App Store certificates)
type("appstore")

# Path to your API key
api_key_path("./api_key.json")

# App identifiers
app_identifier([
  "com.saoirse.epi"
])
